<!DOCTYPE html>
<html>
<head>
  <title>Rantly</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<header>
  <div class="container">
    <div class="logo">
      <%= link_to "Rant.ly", root_path %>
    </div>
    <div class="login">
      <%= link_to "Login", login_path %>
    </div>
    <div class="join">
      <%= link_to "Join", new_user_path %>
    </div>

  </div>
</header>
<%= yield %>



</body>
</html>
