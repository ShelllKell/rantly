<h1>Register</h1>

<%= form_for @user do |f| %>
<div>
  <%= f.label :username %>
  <%= f.text_field :username, autofocus: true %>
</div>
<div>
  <%= f.label :password %>
  <%= f.password_field :password %>
</div>

  <div>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>

  <div>
    <%=f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>

  <div>
    <%= f.label :bio %>
    <%= f.text_field :bio %>
  </div>

  <div>
    <%= f.submit "REGISTER" %>
  </div>


<% end %>
